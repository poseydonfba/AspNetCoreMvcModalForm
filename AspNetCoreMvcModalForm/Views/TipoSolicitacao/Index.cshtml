@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery antiforgeryProvider
@model IEnumerable<AspNetCoreMvcModalForm.Business.Models.TipoSolicitacao>

@{
    ViewData["Title"] = "Tipo de Solicitações";
}

@await Html.PartialAsync("_ViewAll", Model)

<input id="requestToken" type="hidden" value="@antiforgeryProvider.GetAndStoreTokens(this.Context).RequestToken" />

@section Scripts{
    <partial name="_ValidationScriptsPartial" />
    <script src="~/js/TipoSolicitacao.js" asp-append-version="true"></script>
}
